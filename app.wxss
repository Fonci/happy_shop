/**app.wxss**/
@import './styles/iconfont.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page,
view,
text,
navigator,
image {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

page {
  --themeColor: #ff2d4a;
}
image{
  width:100%;
}
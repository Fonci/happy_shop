swiper {
  width: 100%;
  height: 340rpx;
}
swiper image {
  width: 100%;
  height: 100%;
}
.item_cata {
  display: flex;
  padding: 20rpx;
}
.item_cata navigator {
  flex: 1;
}
.item_cata navigator image {
  width: 100%;
}
.index_floor .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_list {
  overflow: hidden;
}
.index_floor .floor_list navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_list navigator:nth-child(2),
.index_floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid white;
}
.index_floor .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid white;
}
.index_floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
